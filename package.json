{
  "name": "noboxout-orm",
  "version": "0.0.1",
  "description": "Noboxout ORM (object relational mapper) with MYSQL backend",
  "main": "./index.js",
  "keywords": [
    "ORM",
    "database",
    "mysql",
    "transaction",
    "model"
  ],
  "directories": {
    "lib": ".",
    "test": "test"
  },
  "scripts": {
    "test": "tap test/test-norm-*.js",
    "coverage": "node test/code-coverage.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/llafuente/noboxout-orm.git"
  },
  "author": {
    "name": "Luis Lafuente",
    "email": "llafuente@noboxout.com",
    "url": "http://www.noboxout.com"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.4"
  },
  "dependencies": {
    "function-enhancements": "*",
    "array-enhancements": "*",
    "object-enhancements": "*",
    "node-class": "*",
    "winston": "*",
    "mysql" : "*"
  },
  "devDependencies": {
    "tap": "~0.4.4",
    "ass": "0.0.4"
  },
  "jshintConfig": {
    "white": true,
    "curly": true,
    "eqeqeq": true,
    "indent": 4,
    "asi": false,
    "boss": false,
    "browser": true,
    "devel": true,
    "maxdepth": 4,
    "maxstatements": 25,
    "maxcomplexity": 6,
    "eqnull": true,
    "es5": true,
    "evil": false,
    "immed": false,
    "jquery": true,
    "latedef": false,
    "laxcomma": true,
    "laxbreak": true,
    "newcap": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "predef": [
      "log"
    ],
    "quotmark": "double",
    "smarttabs": false,
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  }
}