{
  "name": "noboxout-orm",
  "version": "0.0.1",
  "description": "Noboxout ORM",
  "main": "./index.js",
  "keywords": [
    "ORM"
  ],
  "directories": {
    "lib": ".",
    "test": "test"
  },
  "scripts": {
    "test": "tap test/test-*.js",
    "coverage": "node test/code-coverage.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/llafuente/noboxout-orm.git"
  },
  "author": {
    "name": "Luis Lafuente",
    "email": "llafuente@noboxout.com",
    "url": "http://www.noboxout.com"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.4"
  },
  "dependencies": {
    "function-enhancements": "*",
    "array-enhancements": "*",
    "object-enhancements": "*",
    "node-class": "*",
    "winston": "*",
    "mysql" : "*"
  },
  "devDependencies": {
    "tap": "~0.4.4",
    "ass": "0.0.4"
  },
  "jshintConfig": {
      "asi": true,
      "boss": true,
      "browser": true,
      "curly": true,
      "devel": true,
      "maxdepth": 4,
      "maxstatements": 25,
      "maxcomplexity": 6,
      "eqeqeq": true,
      "eqnull": true,
      "es5": true,
      "evil": false,
      "immed": false,
      "jquery": true,
      "latedef": false,
      "laxcomma": true,
      "laxbreak": true,
      "newcap": true,
      "node": true,
      "noempty": true,
      "nonew": true,
      "predef": [
        "log"
      ],
      "quotmark": "double",
      "smarttabs": true,
      "strict": false,
      "trailing": false,
      "undef": true,
      "unused": true
    }
}
